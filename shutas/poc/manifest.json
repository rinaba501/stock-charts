{
    "manifest_version": 3,
    "name": "Stock Chart POC",
    "version": "0.3",
    "icons": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    },
    "permissions": [
      "activeTab", "scripting", "contextMenus", "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
      },
    "background": {
      "service_worker": "background.js"
    }  
  }